    <script type="text/javascript">
    $(function() {

        //如果没有租赁信息.空白界面

        // $('body').addClass("setcolor");

        // $('.voidmessage').css('display', 'block');

        // $('.myhouseArtical').css('display', 'none');
        
        $('#headpull').click(function() {
            var displaystaty = $('.housepull').css('display');
            if (displaystaty == 'none') {
                $('.housepull').css('display', 'block');
            } else {
                $('.housepull').css('display', 'none');
            }
        });

        $('.itemclick').click(function(e) {
            var that = $(this);
            $('.pullUl li a').each(function() {
                var _this = $(this);
                var classname = _this.attr('class');
                if (classname.indexOf('curr') > 0) {
                    _this.attr('class', 'itemclick');
                }
            })
            that.addClass('curr');
            var n = e.currentTarget.innerHTML;
            console.log(n);
            $('#headpull').html(n+'<span></span>');

            that.parent('li').parent('.pullUl').parent('.housepull').css('display', 'none');

        })
    })
    </script>
